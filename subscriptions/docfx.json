{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/visualstudio-docs.fr-fr",
      "feedback_product_url": "https://developercommunity.visualstudio.com/",
      "breadcrumb_path": "~/_breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "manager": "lank",
      "ms.topic": "conceptual",
      "ms.prod": "visual-studio-family",
      "ms.technology": "vs-subscriptions",
      "disable_contribution_on_translation_quality": [
        "MT",
        "MTE75",
        "MTE95",
        "Blank"
      ],
      "searchScope": [
        "VS Subscriptions"
      ]
    },
    "fileMetadata": {
        "disable_contribution_on_translation_quality": {
        "kb-support/*.md": ["HT","MT","MTE75","MTE95","Blank"],
        "kb-support/**/*.md": ["HT","MT","MTE75","MTE95","Blank"]
      },
      "feedback_system": {
        "kb-support/*.md": "None",
        "kb-support/**/*.md": "None"
      }
    },
    "template": [],
    "dest": "subscriptions"
  }
}
